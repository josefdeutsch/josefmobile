<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.josef.josefmobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.josef.mobile.ErrorActivity"
            android:parentActivityName="com.josef.mobile.MainActivity"></activity>
        <activity
            android:name="com.josef.mobile.MainActivity"
            android:parentActivityName="com.josef.mobile.SplashActivity" />
        <activity android:name="com.josef.mobile.free.PresenterActivity" />
        <activity android:name="com.josef.mobile.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>